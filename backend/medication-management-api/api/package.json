{
  "name": "medication_management_api",
  "version": "1.0.0",
  "description": "Medication Management API",
  "main": "app.js",
  "author": "Thomas Bowen",
  "license": "MIT",
  "scripts": {
    "unit": "jest",
    "lint": "eslint '*.ts' --quiet --fix",
    "compile": "tsc",
    "test": "npm run compile && npm run unit",
    "prisma:generate": "prisma generate",
    "prisma:makemigrations": "prisma migrate dev --name migration",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "seed": "tsx src/scripts/seed.ts",
    "dev": "tsx app.ts",
    "build-with-prisma": "prisma generate && tsc && node copy-prisma-engines.js",
    "sync-schemas": "node sync-prisma-schemas.js",
    "build-layer": "cd ../layers/prisma && npm run build && cd ../../",
    "build": "npm run prisma:generate && tsc && node copy-prisma-engines.js && npm run sync-schemas && npm run build-layer && cd .. && sam build && cd api",
    "dev:sam": "cd .. && node api/start-sam.js",
    "spec": "tsoa spec",
    "prebuild": "npm run spec"
  },
  "dependencies": {
    "@prisma/client": "^5.10.0",
    "aws-sdk": "^2.1499.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.3.1",
    "esbuild": "^0.14.14",
    "jsonwebtoken": "^9.0.2",
    "reflect-metadata": "^0.2.2",
    "tsoa": "^6.6.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.2.0",
    "@types/aws-lambda": "^8.10.92",
    "@types/bcryptjs": "^2.4.6",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.2.0",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^18.19.78",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "fs-extra": "^11.3.0",
    "jest": "^29.2.1",
    "prettier": "^2.5.1",
    "prisma": "^5.10.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsx": "^4.19.3",
    "typescript": "^4.8.4"
  },
  "npm": {
    "onlyBuiltDependencies": [
      "@prisma/client",
      "@prisma/engines",
      "aws-sdk",
      "esbuild",
      "prisma"
    ]
  }
}
